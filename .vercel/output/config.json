{
	"version": 3,
	"routes": [
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/_server-islands/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/_image/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/auth/login/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/auth/logout/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/auth/register/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/events/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/profile/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/recipes/generate/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/recipes/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/recipes/?$",
			"dest": "_render"
		},
		{
			"src": "^/app/recipes/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/app/?$",
			"dest": "_render"
		},
		{
			"src": "^/auth/forgot/?$",
			"dest": "_render"
		},
		{
			"src": "^/auth/reset/?$",
			"dest": "_render"
		},
		{
			"src": "^/login/?$",
			"dest": "_render"
		},
		{
			"src": "^/profile/?$",
			"dest": "_render"
		},
		{
			"src": "^/$",
			"dest": "_render"
		}
	]
}